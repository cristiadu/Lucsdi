    <!-- Find a Tutor -->
    <div id="content-minor" class="container">
        <div class="row">
            <div class="col-xs-12 col-md-12">
                <h3 class="page-header">Meet with a tutor</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-md-12">
                <form role="form">
                      <div class="form-group">
                        <label for="course-name">Course name</label>
                        <input type="text" class="form-control" id="course-name" name="course-name" placeholder="Enter course name Ex) COGS">
                      </div>
                      <div class="form-group">
                        <label for="course-number">Course number</label>
                        <input type="text" class="form-control" id="course-number" name="course-number" placeholder="Enter course number Ex) 120">
                      </div>
                      <div class="form-group">
                        
                        <select class="form-control" name="language-tutor" id="language-tutor">
                            <option value="" disabled="disabled" selected="selected">Find a Tutor who Speaks:</option>
                            <option value="English">English</option>
                            <option value="Arabic">Arabic</option>
                            <option value="Chinese (Mandarin)">Chinese (Mandarin)</option>
                            <option value="French">French</option>
                            <option value="German">German</option>
                            <option value="Italian">Hindi</option>
                            <option value="Italian">Italian</option>
                            <option value="Japanese">Japanese</option>
                            <option value="Korean">Korean</option>
                            <option value="Spanish">Spanish</option>
                        </select>
                      </div>
                        <input class="btn btn-primary" id="btn-scheduleTutor" type="submit" value="Schedule Appointment">
                      </form>
                    
         
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-md-12">
                <hr color="blue"> 
                <h3 class="page-header">Meet with an advisor</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-md-12">
                <form role="form">
                      <div class="form-group">
                        <select class="form-control" id="department-ad" name="department-ad">
                            <option value="" disabled="disabled" selected="selected">Please Select a Department</option>
                            <option value="Anthropology">Anthropology</option>
                            <option value="Biology">Biology</option>
                            <option value="Bioengineering">Bioengineering</option>
                            <option value="Chemistry">Chemistry</option>
                            <option value="Classical Studies">Classical Studies</option>
                            <option value="Cognitive Science">Cognitive Science</option>
                            <option value="Computer Science">Computer Science</option>
                            <option value="Economics">Economics</option>
                            <option value="Engineering">Engineering</option>
                            <option value="History">History</option>
                            <option value="Literature">Literature</option>
                            <option value="Linguistics">Linguistics</option>
                            <option value="Mathematics">Mathematics</option>
                            <option value="Music">Music</option>
                            <option value="Physics">Physics</option>
                            <option value="Political Science">Political Science</option>
                            <option value="Psychology">Psychology</option>
                            <option value="Sociology">Sociology</option>
                            <option value="Visual Arts">Visual Arts</option>
                        </select>
                      </div>
                      <div class="form-group">
                        <select class="form-control" id="college-ad" name="college-ad">
                            <option value="" disabled="disabled" selected="selected">Please Select a College</option>
                            <option value="Eleanor Roosevelt">Eleanor Roosevelt</option>
                            <option value="Marshall">Marshall</option>
                            <option value="Muir">Muir</option>
                            <option value="Revelle">Revelle</option>
                            <option value="Sixth">Sixth</option>
                            <option value="Warren">Warren</option>
                        </select>
                      </div>
                      <div class="form-group">
                        
                        <select class="form-control" name="int-ad" id="int-ad">
                            <option value="" disabled="disabled" selected="selected">Find an International Advisor</option>
                            <option value="Advisor1">Graciela Casas-Silva</option>
                            <option value="Advisor2">Gabriela Hoffmann, ITTP Coordinator</option>
                            <option value="Advisor3">Marie Manlapaz, Orientation Co-Coordinator</option>
                            <option value="Advisor4">Laura Page</option>
                            <option value="Advisor5">Ricky Paniagua</option>
                            <option value="Advisor6">Hannah Reese, International Leaders Coordinator</option>
                            <option value="Advisor7">Lisa Swid, Triton Trekkers Coordinator</option>
                            <option value="Advisor8">Shelly Taskin</option>
                            <option value="Advisor9">Carol Robertson</option>
                        </select>
                      </div>
                      <div class="form-group">
                        <label for="date">Date</label>
                        <input type="text" class="form-control" id="date" name="date" placeholder="Enter date">
                      </div>
                      <div class="form-group">
                        <select class="form-control" id="hour-ad" name="hour-ad">
                            <option value="" disabled="disabled" selected="selected">Schedule a Meeting</option>
                            <option value="Time1">9:00 am</option>
                            <option value="Time2">9:30 am</option>
                            <option value="Time3">10:00 am</option>
                            <option value="Time4">10:30 am</option>
                            <option value="Time5">11:00 am</option>
                            <option value="Time6">11:30 am</option>
                            <option value="Time7">12:00 pm</option>
                            <option value="Time8">12:30 pm</option>
                            <option value="Time9">1:00 pm</option>
                            <option value="Time10">1:30 pm</option>
                            <option value="Time11">2:00 pm</option>
                            <option value="Time12">2:30 pm</option>
                            <option value="Time13">3:00 pm</option>
                            <option value="Time14">3:30 pm</option>
                            <option value="Time15">4:00 pm</option>
                        </select>
                      </div>
                      <!-- <div class="form-group">
                        <label for="name-st">Your name</label>
                        <input type="text" class="form-control" id="name-st" name="name-st" placeholder="Enter your name">
                      </div>
                      <div class="form-group">
                        <label for="email-st">Your email</label>
                        <input type="email" class="form-control" id="email-st" name="email-st" placeholder="Enter your email">
                      </div> -->
                      <input class="btn btn-primary" id="btn-scheduleAdvisor" type="submit" value="Schedule Appointment">
                  </form>
                      <!-- <a class="btn btn-primary" href="confirmation-advisor">Schedule Appointment</a><br> -->
                     
                      <br>
                      <br>
                <!--</form>-->
            </div>
        </div>
             
    </div>
